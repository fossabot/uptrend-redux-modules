    "tcomb": "^3.2.25"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jest-in-case": "^1.0.2",
    "kcd-scripts": "^0.37.0",
    "kitimat": "^0.1.8",
    "kitimat-jest": "^0.1.8",
    "normalizr": "^3.2.4",
    "redux-mock-store": "^1.5.1"
  },
  "eslintIgnore": ["node_modules", "coverage", "dist"],
  "bugs": {
    "url":
      "https://github.com/uptrend-tech/redux-modules-resource-entities/issues"
